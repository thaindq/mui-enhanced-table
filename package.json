  "dependencies": {
    "classnames": "^2.3.1",
    "lodash": "^4.17.21",
    "react-beautiful-dnd": "^13.1.0"
  },
  "peerDependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.8.0",
